local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local Title = Instance.new("TextLabel")
local CloseButton = Instance.new("TextButton")
local MinimizeButton = Instance.new("TextButton")
local XBox = Instance.new("TextBox")
local YBox = Instance.new("TextBox")
local ZBox = Instance.new("TextBox")
local TeleportButton = Instance.new("TextButton")
local SaveButton = Instance.new("TextButton")
local DeleteButton = Instance.new("TextButton")
local savedPosition = nil

-- Configurações da Interface
ScreenGui.Parent = game.CoreGui
Frame.Parent = ScreenGui
Frame.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
Frame.Size = UDim2.new(0, 200, 0, 200)
Frame.Position = UDim2.new(0.5, -100, 0.5, -100)
Frame.Draggable = true
Frame.Active = true
Frame.BorderSizePixel = 2

Title.Parent = Frame
Title.Text = "Teleport GUI"
Title.Size = UDim2.new(1, 0, 0, 25)
Title.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
Title.TextColor3 = Color3.fromRGB(255, 255, 255)

MinimizeButton.Parent = Frame
MinimizeButton.Text = "-"
MinimizeButton.Size = UDim2.new(0, 25, 0, 25)
MinimizeButton.Position = UDim2.new(1, -50, 0, 0)

CloseButton.Parent = Frame
CloseButton.Text = "X"
CloseButton.Size = UDim2.new(0, 25, 0, 25)
CloseButton.Position = UDim2.new(1, -25, 0, 0)

XBox.Parent = Frame
XBox.PlaceholderText = "X"
XBox.Size = UDim2.new(0.8, 0, 0, 25)
XBox.Position = UDim2.new(0.1, 0, 0.2, 0)

YBox.Parent = Frame
YBox.PlaceholderText = "Y"
YBox.Size = UDim2.new(0.8, 0, 0, 25)
YBox.Position = UDim2.new(0.1, 0, 0.4, 0)

ZBox.Parent = Frame
ZBox.PlaceholderText = "Z"
ZBox.Size = UDim2.new(0.8, 0, 0, 25)
ZBox.Position = UDim2.new(0.1, 0, 0.6, 0)

TeleportButton.Parent = Frame
TeleportButton.Text = "Teleport"
TeleportButton.Size = UDim2.new(0.8, 0, 0, 25)
TeleportButton.Position = UDim2.new(0.1, 0, 0.8, 0)

SaveButton.Parent = Frame
SaveButton.Text = "Salvar"
SaveButton.Size = UDim2.new(0.4, 0, 0, 25)
SaveButton.Position = UDim2.new(0.1, 0, 0.9, 0)

DeleteButton.Parent = Frame
DeleteButton.Text = "Deletar"
DeleteButton.Size = UDim2.new(0.4, 0, 0, 25)
DeleteButton.Position = UDim2.new(0.5, 0, 0.9, 0)

-- Funcionalidades
CloseButton.MouseButton1Click:Connect(function()
    ScreenGui:Destroy()
end)

MinimizeButton.MouseButton1Click:Connect(function()
    Frame.Visible = not Frame.Visible
end)

SaveButton.MouseButton1Click:Connect(function()
    local x = tonumber(XBox.Text)
    local y = tonumber(YBox.Text)
    local z = tonumber(ZBox.Text)
    if x and y and z then
        savedPosition = Vector3.new(x, y, z)
    end
end)

DeleteButton.MouseButton1Click:Connect(function()
    savedPosition = nil
end)

TeleportButton.MouseButton1Click:Connect(function()
    local player = game.Players.LocalPlayer
    if player and player.Character and savedPosition then
        player.Character.HumanoidRootPart.CFrame = CFrame.new(savedPosition)
    end
end)
