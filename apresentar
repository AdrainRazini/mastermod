-- üñºÔ∏è Fun√ß√£o utilit√°ria para criar UI Corner Obs: Aplicar Ui nas frames
local function applyCorner(instance, radius)
	local corner = Instance.new("UICorner")
	corner.CornerRadius = radius or UDim.new(0, 6)
	corner.Parent = instance
end

-- Fun√ß√£o para aplicar contorno neon via UIStroke
local function applyUIStroke(instance, colorName, thickness)
	thickness = thickness or 2
	local stroke = Instance.new("UIStroke")
	stroke.Parent = instance
	stroke.Thickness = thickness
	stroke.LineJoinMode = Enum.LineJoinMode.Round
	stroke.Transparency = 0
	-- Escolhe cor da paleta ou usa branco como fallback
	stroke.Color = colors[colorName] or Color3.new(1, 1, 1)
end

local function applyUIListLayout(instance, padding, sortOrder, alignment)
	local list = Instance.new("UIListLayout")
	list.Parent = instance

	-- Padding entre elementos (UDim ou padr√£o 0)
	list.Padding = padding or UDim.new(0, 0)

	-- Ordem dos elementos
	list.SortOrder = sortOrder or Enum.SortOrder.LayoutOrder

	-- Alinhamento
	list.HorizontalAlignment = alignment or Enum.HorizontalAlignment.Center
end


local function applyRotatingGradientUIStroke(instance, cor1, cor2, cor3)
	cor1 = cor1 or "White"
	cor2 = cor2 or "White"
	cor3 = cor3 or "White"

	local stroke = Instance.new("UIStroke")
	stroke.Thickness = 2
	stroke.Transparency = 0
	stroke.LineJoinMode = Enum.LineJoinMode.Round
	stroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
	stroke.Color = colors.White
	stroke.Parent = instance

	local gradient = Instance.new("UIGradient")
	gradient.Rotation = 0
	gradient.Color = ColorSequence.new({
		ColorSequenceKeypoint.new(0.0, colors[cor1]),
		ColorSequenceKeypoint.new(0.5, colors[cor2]),
		ColorSequenceKeypoint.new(1.0, colors[cor3])
	})
	gradient.Parent = stroke

	-- Anima√ß√£o da rota√ß√£o do gradiente
	local angle = 0
	RunService.RenderStepped:Connect(function()
		if not gradient.Parent then return end
		angle = (angle + 0.5) % 360
		gradient.Rotation = angle
	end)
end

local config = {
	duration = 5,
	loading = false,
	text_loading = false,
	bar_loading = false
}


local function createGUI()
	-- Limpa GUI antiga
	if G1L["ScreenGui"] then
		G1L["ScreenGui"]:Destroy()
	end

	G1L = {} -- Reseta tabela

	-- GUI principal
	G1L["ScreenGui"] = Instance.new("ScreenGui", player:WaitForChild("PlayerGui"))
	G1L["ScreenGui"].Name = NAME_GUI

	G1L["Frame"] = Instance.new("Frame", G1L["ScreenGui"])
	G1L["Frame"].AnchorPoint = Vector2.new(0.5, 0.5)
	G1L["Frame"].Position = UDim2.new(0.5, 0, 0.5, 0)
	G1L["Frame"].Size = UDim2.new(0, 320, 0, 220)
	G1L["Frame"].BackgroundColor3 = Color3.fromRGB(20, 20, 20)
	G1L["Frame"].BackgroundTransparency = 0.15
	G1L["Frame"].BorderSizePixel = 0

	-- Avatar
	G1L["Avatar"] = Instance.new("ImageLabel", G1L["Frame"])
	G1L["Avatar"].Size = UDim2.new(0, 90, 0, 90)
	G1L["Avatar"].Position = UDim2.new(0.5, -45, 0.15, -45)
	G1L["Avatar"].BackgroundTransparency = 1
	G1L["Avatar"].Image = "https://www.roblox.com/headshot-thumbnail/image?userId=1393562880&width=420&height=420&format=png"


	-- Nome
	G1L["NameText"] = Instance.new("TextLabel", G1L["Frame"])
	G1L["NameText"].Size = UDim2.new(0, 400, 0, 50)
	G1L["NameText"].Position = UDim2.new(0.5, -200, 0.55, -25)
	G1L["NameText"].Text = Owner.."\nAdrianRazini"
	G1L["NameText"].TextColor3 = Color3.fromRGB(255, 255, 255)
	G1L["NameText"].BackgroundTransparency = 1
	G1L["NameText"].TextScaled = true
	G1L["NameText"].Font = Enum.Font.GothamBold
	G1L["NameText"].TextStrokeTransparency = 0.3

	-- Loading Frame
	G1L["LoadingFrame"] = Instance.new("Frame", G1L["Frame"])
	G1L["LoadingFrame"].Size = UDim2.new(0, 300, 0, 50)
	G1L["LoadingFrame"].Position = UDim2.new(0.5, -150, 0.85, -25)
	G1L["LoadingFrame"].BackgroundTransparency = 1

	-- Loading Text
	G1L["LoadingLabel"] = Instance.new("TextLabel", G1L["LoadingFrame"])
	G1L["LoadingLabel"].Size = UDim2.new(1, 0, 0.5, 0)
	G1L["LoadingLabel"].Text = "Loading..."
	G1L["LoadingLabel"].TextColor3 = Color3.fromRGB(200, 200, 255)
	G1L["LoadingLabel"].BackgroundTransparency = 1
	G1L["LoadingLabel"].TextScaled = true
	G1L["LoadingLabel"].Visible = false
	G1L["LoadingLabel"].Font = Enum.Font.Gotham

    G1L["SpinnerFrame"] = Instance.new("Frame", G1L["LoadingFrame"])
	G1L["SpinnerFrame"].Size = UDim2.new(0, 30, 0, 30)
	G1L["SpinnerFrame"].Position = UDim2.new(0.5, 0, 0.5, 0)	
	G1L["SpinnerFrame"].BackgroundTransparency = 1
	
	-- Spinner
	G1L["Spinner"] = Instance.new("ImageLabel", G1L["SpinnerFrame"])
	G1L["Spinner"].AnchorPoint = Vector2.new(0.5, 0.5)
	G1L["Spinner"].Position = UDim2.new(0.5, 0, 0.5, 0)
	G1L["Spinner"].Size = UDim2.new(0, 30, 0, 30)
	G1L["Spinner"].BackgroundTransparency = 1
	G1L["Spinner"].Visible = false
	G1L["Spinner"].Image = icons.fa_bx_loader
	
	G1L["Bar_Max"] = Instance.new("Frame", G1L["LoadingFrame"])
	G1L["Bar_Max"].Size = UDim2.new(0, 300, 0, 5)
	G1L["Bar_Max"].Position = UDim2.new(0.5, -150, 1, -3)
	G1L["Bar_Max"].Visible = false
	G1L["Bar_Max"].BackgroundTransparency = 1
	G1L["Bar_Max"].BorderSizePixel = 1
	G1L["Bar_Max"].ZIndex = 100
	
	G1L["Bar"] = Instance.new("Frame", G1L["Bar_Max"])
	G1L["Bar"].Size = UDim2.new(0, 0, 1, 0) -- largura inicial 0, altura cheia
	G1L["Bar"].BackgroundColor3 = Color3.fromRGB(0, 47, 255)
	G1L["Bar"].BackgroundTransparency = 0  -- garante que apare√ßa
	G1L["Bar"].BorderSizePixel = 0

	-- Define a ordem desejada
	G1L["Avatar"].LayoutOrder = 1
	G1L["NameText"].LayoutOrder = 2
	G1L["LoadingFrame"].LayoutOrder = 3

	-- Dentro do LoadingFrame, podemos ordenar Text e Bar
	G1L["LoadingLabel"].LayoutOrder = 1
	G1L["Bar_Max"].LayoutOrder = 2

	-- Agora aplicamos UIListLayout ao Frame principal
	applyUIListLayout(G1L["Frame"], UDim.new(0, 10), Enum.SortOrder.LayoutOrder, Enum.HorizontalAlignment.Center)

	-- Se quiser organizar os elementos dentro do LoadingFrame tamb√©m
	applyUIListLayout(G1L["LoadingFrame"], UDim.new(0, 5), Enum.SortOrder.LayoutOrder, Enum.HorizontalAlignment.Center)



	applyCorner(G1L["Frame"], UDim.new(0, 12))
	applyCorner(G1L["LoadingFrame"], UDim.new(0, 10))
	applyCorner(G1L["Bar_Max"], UDim.new(0, 10))
	applyCorner(G1L["Bar"], UDim.new(0, 4))
	
	
	applyUIStroke(G1L["LoadingLabel"], "Accent", 1)
		
	applyRotatingGradientUIStroke(G1L["Bar_Max"], "Accent", "Blue", "Purple")
	applyRotatingGradientUIStroke(G1L["Frame"], "Accent", "Blue", "Purple")


end

createGUI() -- Cria GUI inicialmente



--======================================================--
local StarterGui = game:GetService("StarterGui")

function Notification(icon, text, time)
	-- Garante que s√≥ n√∫meros do assetid ser√£o usados
	local cleanIcon = tostring(icon):gsub("%D", "") -- remove tudo que n√£o for n√∫mero

	StarterGui:SetCore("SendNotification", { 
		Title = GITHUB_REPO;
		Text = text or "";
		Icon = "rbxthumb://type=Asset&id="..cleanIcon.."&w=150&h=150";
		Duration = time or 5;
	})
end

--======================================================--

Notification(icons.fa_bx_code_start, "Hello: " .. player.Name, 10)

-- Fun√ß√µes
local function executarScript()
	local scriptUrl = "https://raw.githubusercontent.com/AdrainRazini/mastermod/refs/heads/main/mod/Mastermods"
	local success, err = pcall(function()
		local source = game:HttpGet(scriptUrl)
		loadstring(source)()
	end)

	if not success then
		warn("[MasterMods] ‚ùå N√£o foi poss√≠vel executar o script remoto.")
		warn("[MasterMods] Motivo: " .. tostring(err))
		print("[MasterMods] üîí Iniciando em modo seguro (sem executor ativado).")
	else
		print("[MasterMods] ‚úÖ Script remoto executado com sucesso.")
	end
end


-- Atualiza visuais conforme estado atual
local function updateLoadingVisuals()
	G1L["LoadingLabel"].Visible = config.text_loading
	G1L["SpinnerFrame"].Visible = config.spinner_frame
	G1L["Spinner"].Visible = config.loading
	G1L["Bar_Max"].Visible = config.bar_loading
end

-- Escolha

local lastEscolha

local function Estado(input)
	local opcoes = {"text_loading", "loading", "bar_loading"}
	local escolha

	if input and table.find(opcoes, input) then
		escolha = input
	else
		repeat
			escolha = opcoes[math.random(1, #opcoes)]
		until escolha ~= lastEscolha
	end

	lastEscolha = escolha

	-- Reseta todas as op√ß√µes
	config.text_loading = false
	config.loading = false
	config.bar_loading = false

	-- Ativa a escolhida
	config[escolha] = true

end





local function startSpinner()
	-- Desliga se n√£o estiver ativo
	if not config.loading then
		G1L["Spinner"].Visible = false
		G1L["SpinnerFrame"].Visible = false
		return
	end

	-- Mostra o spinner
	G1L["Spinner"].Visible = true
	G1L["SpinnerFrame"].Visible = true

	local startTime = tick()

	-- Gira o spinner enquanto durar o tempo
	task.spawn(function()
		while tick() - startTime < config.duration and config.loading do
			G1L["Spinner"].Rotation = G1L["Spinner"].Rotation + 10
			task.wait(0.016) -- ~60 FPS
		end
		G1L["Spinner"].Rotation = 0 -- opcional: reseta a rota√ß√£o no final
		G1L["Spinner"].Visible = false -- opcional: esconde ao terminar
		G1L["SpinnerFrame"].Visible = false
	end)
end





-- Texto animado
local function animateText()
	if not config.text_loading then
		G1L["LoadingLabel"].Visible = false
		return
	end

	G1L["LoadingLabel"].Visible = true
	task.spawn(function()
		local points = 0
		while G1L["LoadingLabel"] and G1L["LoadingLabel"].Parent do
			G1L["LoadingLabel"].Text = "Loading" .. string.rep(".", points)
			points = (points + 1) % 4
			task.wait(0.5)
		end
	end)
end

-- Barra de progresso
local function animateBar()
	if not config.bar_loading then
		G1L["Bar_Max"].Visible = false
		return
	end

	G1L["Bar_Max"].Visible = true  -- precisa estar vis√≠vel
	G1L["Bar"].Size = UDim2.new(0, 0, 1, 0) -- reset largura
	local startTime = tick()

	task.spawn(function()
		while tick() - startTime < config.duration do
			local progress = (tick() - startTime) / config.duration
			G1L["Bar"].Size = UDim2.new(progress, 0, 1, 0)
			task.wait(0.03)
		end
		G1L["Bar"].Size = UDim2.new(1, 0, 1, 0) -- completa a barra
	end)
end


-- Fun√ß√£o para iniciar tudo de uma vez
local function startLoading(input)
	local Tempo = config.duration

	updateLoadingVisuals()
	--Estado("loading")
	--Estado("bar_loading")
	--Estado("text_loading")
	Estado()
	startSpinner()
	animateText()
	animateBar()

	wait(Tempo)
	executarScript()

	if input then
		if G1L["ScreenGui"] then
			G1L["ScreenGui"]:Destroy()
		end
	end
end

-- Chama a inicializa√ß√£o
--startLoading(false)  -- para testar sem destruir a GUI
startLoading(true)   -- para testar e destruir a GUI


-- Recria GUI no respawn
player.CharacterAdded:Connect(function()
	if G1L["ScreenGui"] then
		G1L["ScreenGui"]:Destroy()
	end
	createGUI()
	startLoading()
end)


